{
  "name": "play_launch_web_ui",
  "version": "0.4.0",
  "description": "Web UI linting tools for play_launch",
  "private": true,
  "scripts": {
    "lint": "npm run lint:html && npm run lint:css && npm run lint:js && npm run typecheck",
    "typecheck": "tsc --noEmit",
    "lint:html": "htmlhint src/play_launch/src/web/assets/index.html",
    "lint:css": "node scripts/lint-inline-css.js",
    "lint:js": "node scripts/lint-inline-js.js",
    "lint:fix": "node scripts/lint-inline-js.js --fix"
  },
  "devDependencies": {
    "eslint": "^8.56.0",
    "htmlhint": "^1.1.4",
    "stylelint": "^16.1.0",
    "stylelint-config-standard": "^36.0.0",
    "typescript": "^5.7.0"
  }
}
